{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "dtypesHelp": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "dtypesStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        },
        "help": {
          "$ref": "#/definitions/dtypesHelp"
        },
        "info": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    },
