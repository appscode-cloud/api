      "type": "object",
      "properties": {
        "aws_fingerprint": {
          "type": "string"
        },
        "openssh_fingerprint": {
          "type": "string"
        },
        "private_key": {
          "type": "string"
        },
        "public_key": {
          "type": "string"
        }
      }
    },
    "sshSecureShellGetRequest": {
      "type": "object",
      "properties": {
        "cluster_name": {
          "type": "string"
        },
        "cluster_namespace": {
          "type": "string"
        },
        "instance_name": {
          "type": "string"
        },
        "jenkins_namespace": {
          "type": "string"
        }
      }
